import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
         Scanner sc=new Scanner(System.in);
        int x1,y1,x2,y2;
        x1=sc.nextInt();
       y1=sc.nextInt();
        x2=sc.nextInt();
        y2=sc.nextInt();
        float X=(float)(x1+x2)/2;
        float Y=(float)(y1+y2)/2;
        System.out.println("Binoy's house is located at ("+X+","+Y+")");
    }
}
