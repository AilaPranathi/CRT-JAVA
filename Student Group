import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n,a;
        n=sc.nextInt();
        a=n/4;
        if(n>100 && n<200)
        {
            System.out.println(a+"\n"+a+"\n"+a+"\n"+(a+n%4));
        }
        else
        {
           System.out.println("INVALID INPUT");
        }
    }
}
