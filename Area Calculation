import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int a,b,c;
        float d;
        a=s.nextInt();
        b=s.nextInt();
        c=s.nextInt();
        d=s.nextFloat();
        System.out.println(a*a);
         System.out.println(b*c);
        double n=3.14f*d*d;
         System.out.printf("%.2f",n);
        
    }
}
